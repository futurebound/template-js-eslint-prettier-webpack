{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "single"]
  }
}
